<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 어떤 DAO에서 사용하는지 -->
<mapper namespace="MT.model.ReviewDAO">

	<insert id="insert"
	parameterType="ReviewVO">
		insert into review
		values(
		review_seq.nextval,
		#{REST_ID},
		#{REVIEW_LEVEL},
		#{REVIEW_CONTENT},
		#{MEMBER_ID},
		sysdate
		)
	</insert>
	

</mapper>